@model IEnumerable<Inventario2.Models.Cliente>

@{
    ViewBag.Title = "Index";
}

<h2>Principal Clientes</h2>

<p>
    @Html.ActionLink("Crear nuevo cliente", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Cli_Nom)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cli_Dir)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cli_Tel)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cli_Cor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cli_Usu)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cli_Con)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cli_Est)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Cli_Nom)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cli_Dir)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cli_Tel)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cli_Cor)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cli_Usu)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cli_Con)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cli_Est)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.Cli_id }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.Cli_id }) |
            @Html.ActionLink("Borrar", "Delete", new { id=item.Cli_id })
        </td>
    </tr>
}

</table>
